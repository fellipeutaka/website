{
  "cSpell.words": [
    "biomejs",
    "codepen",
    "commitlint",
    "fontsource",
    "ratelimit",
    "sonner",
    "superjson",
    "tailwindcss",
    "trpc",
    "turborepo",
    "upstash",
    "upvote",
    "Upvoted",
    "Upvotes"
  ],
  "editor.defaultFormatter": "biomejs.biome",
