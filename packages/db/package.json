{
  "name": "@utaka/db",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "private": true,
  "scripts": {
    "tsx": "tsx --env-file=../../.env",
    "drizzle": "node --env-file=../../.env ./node_modules/drizzle-kit/bin.cjs",
    "generate": "pnpm run drizzle generate",
    "migrate": "pnpm run tsx src/migrate.ts",
    "seed": "pnpm run tsx src/seed/index.ts",
    "drop": "pnpm run drizzle drop",
    "pull": "pnpm run drizzle introspect",
    "push": "pnpm run drizzle push",
    "studio": "pnpm run drizzle studio",
    "check": "pnpm run drizzle check",
    "metadata": "pnpm run drizzle up",
    "start": "docker compose up -d",
    "stop": "docker compose stop",
    "delete": "docker compose down -v"
  },
  "dependencies": {
    "@utaka/env": "workspace:*",
    "drizzle-orm": "^0.33.0",
    "postgres": "^3.4.4"
  },
  "devDependencies": {
    "@utaka/tsconfig": "workspace:*",
    "drizzle-kit": "^0.24.2",
    "next-auth": "beta",
    "pg": "^8.13.0",
    "tsx": "^4.19.1"
  }
}
